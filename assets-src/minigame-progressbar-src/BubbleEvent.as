package {    import flash.events.Event;    public class BubbleEvent extends Event {        public static const BUBBLE:String = "bubble";        public var index:int;        public function BubbleEvent(type:String, in_index:int) {            this.index = in_index;            super(type, true);        }        override public function clone():Event {            var event:BubbleEvent = new BubbleEvent(type, this.index);            return event;        }    }}